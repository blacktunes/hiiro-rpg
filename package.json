{
  "name": "game",
  "version": "1.0.0",
  "main": "src/main.js",
  "scripts": {
    "watch": "cross-env NODE_ENV=development nodemon --watch build --watch src --exec electron .",
    "start": "cross-env NODE_ENV=development electron .",
    "build": "electron-packager src Hiiro --overwrite --icon=src/icon/icon.ico --asar --out=dist",
    "build:src": "electron-packager src Hiiro --overwrite --icon=src/icon/icon.ico --out=dist"
  },
  "author": "Blacktunes",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hrk-dev/game.git"
  },
  "keywords": [
    "rpgmakermv",
    "electron"
  ],
  "bugs": {
    "url": "https://github.com/hrk-dev/game/issues"
  },
  "homepage": "https://github.com/hrk-dev/game#readme",
  "devDependencies": {
    "cross-env": "^7.0.3",
    "electron": "^12.0.7",
    "electron-packager": "^15.2.0",
    "nodemon": "^2.0.7"
  }
}
